{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Poetry Debug",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/src/crewagents/main.py",
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}/src",
        "env": {
          "POETRY_ACTIVE": "1"
        },
        "envFile": "${workspaceFolder}/.env",
        "justMyCode": false,
        "subProcess": true,
        "showReturnValue": true
      }
    ]
}
  